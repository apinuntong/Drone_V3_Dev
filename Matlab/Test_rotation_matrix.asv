clc
clear all
% /* Fill the rotation matrix, using Euler angles */
A =[];

M_PIf = PI;

xAnglev = 0;
yAngle = 0;
zAngle = 0;


cx = cos(xAngle*M_PIf/180.0);
cy = cos(yAngle*M_PIf/180.0);
cz = cos(zAngle*M_PIf/180.0);

sx = sinf(xAngle*M_PIf/180.0);
sy = sinf(yAngle*M_PIf/180.0);
sz = sinf(zAngle*M_PIf/180.0);

A(1) = cz*cy;
A(2) = cz*sy*sx-sy*cx; 
A(3) = cz*sy*cx+sz*sx;
A(4) = sz*cy;
A(5) = sz*sy*sx+cz*cx;
A(6) = sz*sy*cx-cz*sx;
A(7) = -sy;
A(8) = cy*sx;
A(9) = cy*cx;